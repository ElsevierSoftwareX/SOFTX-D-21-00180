{
    "source": {
      "include": ["temporal-plugins-server/","./TA_BPMN_editor/app/temporal-modeler/temporal-plugins-client/", "serverNodeJS.js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    },
    "opts": {
      "recurse": true,
      "destination": "./docs/",
      "template": "./docs_custom_template",
      "tutorials": "./tutorials",
      "readme": "./tutorials/index.md"
    }
  }
